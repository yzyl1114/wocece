{
  "files": {
    "core": ["js/calculation.js", "js/report-components.js", "js/result-manager.js"],
    "app": ["js/app.js", "js/template-engine.js", "js/storage.js"],
    "payment": ["js/payment.js"],
    "charts": ["js/chart-renderer.js"],
    "share": ["js/share.js"]
  },
  "options": {
    "compact": true,
    "controlFlowFlattening": true,
    "controlFlowFlatteningThreshold": 0.75,
    "deadCodeInjection": false,
    "debugProtection": false,
    "disableConsoleOutput": true,
    "identifierNamesGenerator": "hexadecimal",
    "log": false,
    "numbersToExpressions": true,
    "renameGlobals": false,
    "selfDefending": true,
    "simplify": true,
    "splitStrings": true,
    "splitStringsChunkLength": 10,
    "stringArray": true,
    "stringArrayEncoding": ["rc4"],
    "stringArrayIndexShift": true,
    "stringArrayRotate": true,
    "stringArrayShuffle": true,
    "stringArrayThreshold": 0.75,
    "transformObjectKeys": true,
    "unicodeEscapeSequence": false,
    "reservedNames": [
      "init", "calculate", "render", "showResult", "loadTest",
      "startTest", "submitAnswer", "getScore", "showReport",
      "pay", "verifyPayment", "api", "$", "jQuery",
      "AlipayJSBridge", "WeixinJSBridge", "wx"
    ],
    "reservedStrings": [
      "wocece", "test", "question", "answer", "result",
      "payment", "alipay", "wechat", "success", "error",
      "score", "report", "category", "price", "questions_count"
    ]
  }
}
